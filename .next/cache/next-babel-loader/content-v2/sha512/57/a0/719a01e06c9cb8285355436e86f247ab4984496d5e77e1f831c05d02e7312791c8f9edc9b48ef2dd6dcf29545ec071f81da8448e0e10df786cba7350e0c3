er ${token}`\n      }\n    }).then(res => {\n      dispatch(deletequizSuccess(filtereddata));\n    }).catch(err => {\n      var errorData;\n\n      if (err.response.data.code == 401) {\n        handle401();\n      }\n\n      if (err.response != null) {\n        errorData = err.response.data.message;\n      } else {\n        errorData = err.message;\n      }\n\n      dispatch(allquizFail(errorData));\n    });\n  };\n};","map":{"version":3,"sources":["/Users/eyasu/Desktop/leanbits/Heineken-SalesTraining-Dashboard/store/Quiz/quizAction.js"],"names":["axios","URLst","handle401","actionTypes","allquizPending","type","QUIZ_PENDING","isPending","allquizSuccess","data","QUIZ_SUCCESS","results","count","totalResults","quizCreateSuccess","CREATE_QUIZ_SUCCESS","updatequizSuccess","UPDATE_QUIZ_SUCCESS","deletequizSuccess","DELETE_QUIZ_SUCCESS","allquizFail","error","QUIZ_FAILED","getAllQuizSuccess","limit","page","token","localStorage","getItem","dispatch","method","url","headers","Authorization","then","res","console","log","catch","err","errorData","response","code","message","quizCreate","formData","post","AllQuizEdit","id","quizs","edited","getState","newData","index","findIndex","av","summary","title","video_source","order","AllQuizDelete","filtereddata","filter","item"],"mappings":"AAAA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,KAAP,IAAgBC,SAAhB,QAAiC,uBAAjC;AACA,OAAO,KAAKC,WAAZ,MAA6B,mBAA7B;AAGA,OAAO,MAAMC,cAAc,GAAG,MAAM;AAClC,SAAO;AACLC,IAAAA,IAAI,EAAEF,WAAW,CAACG,YADb;AAELC,IAAAA,SAAS,EAAE;AAFN,GAAP;AAID,CALM;AAMP,OAAO,MAAMC,cAAc,GAAIC,IAAD,IAAU;AACtC,SAAO;AACLJ,IAAAA,IAAI,EAAEF,WAAW,CAACO,YADb;AAELH,IAAAA,SAAS,EAAE,KAFN;AAGLE,IAAAA,IAAI,EAAEA,IAAI,CAACE,OAHN;AAILC,IAAAA,KAAK,EAAEH,IAAI,CAACI;AAJP,GAAP;AAMD,CAPM;AASP,OAAO,MAAMC,iBAAiB,GAAIL,IAAD,IAAU;AACzC,SAAO;AACLJ,IAAAA,IAAI,EAAEF,WAAW,CAACY,mBADb;AAELN,IAAAA,IAAI,EAAEA;AAFD,GAAP;AAID,CALM;AAOP,OAAO,MAAMO,iBAAiB,GAAIP,IAAD,IAAU;AACzC,SAAO;AACLJ,IAAAA,IAAI,EAAEF,WAAW,CAACc,mBADb;AAELV,IAAAA,SAAS,EAAE,KAFN;AAGLE,IAAAA,IAAI,EA